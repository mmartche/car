from [AcesMod], [TabAces]
where [TabAces].[ativo] = 's' and [AcesMod].[NumTabAces] = [TabAces].[NumTabAces]
order by [AcesMod].[NumMont], [AcesMod].[NumTabAces], [AcesMod].[NumModel], [AcesMod].[AnoMod]

