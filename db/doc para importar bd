
select [Montad].[NumMont], [Montad].[Descr]
from [Montad]
where [Montad].[Ativa]='S'
order by [Montad].[NumMont] desc

select [Linhas].[NumLinha], [Linhas].[NumMont], [Linhas].[Descr]
from [Linhas]
where [Linhas].[Ativo]='S'
order by [Linhas].[NumLinha] desc


